{
    "name": "varunazad/laravel-quicksilver",
    "description": "Blazing-fast query optimization and memory management for Laravel",
    "type": "library",
    "version": "1.0.0",  
    "license": "MIT",
    "keywords": ["laravel", "query", "optimization", "performance", "memory"],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^8.0|^9.0",
        "illuminate/database": "^8.0|^9.0",
        "illuminate/cache": "^8.0|^9.0"
    },
    "require-dev": {
       "orchestra/testbench": "^6.0|^7.0|^8.0",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Varunazad\\Quicksilver\\": "src/",
             "Varunazad\\Quicksilver\\Facades\\": "src/Facades/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Varunazad\\Quicksilver\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Varunazad\\Quicksilver\\QuicksilverServiceProvider"
            ],
            "aliases": {
                "Quicksilver": "Varunazad\\Quicksilver\\Facades\\Quicksilver"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
