{
    "name": "varun/laravel-quicksilver",
    "description": "Blazing-fast query optimization and memory management for Laravel",
    "type": "library",
    "license": "MIT",
    "keywords": ["laravel", "query", "optimization", "performance", "memory"],
    "require": {
        "php": "^8.0 | ^7.4",
        "illuminate/support": "^8.0|^9.0|^10.0|^11.0",
        "illuminate/database": "^8.0|^9.0|^10.0",
        "illuminate/cache": "^8.0|^9.0|^10.0"
    },
    "require-dev": {
       "orchestra/testbench": "^6.0|^7.0|^8.0",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Varun\\Quicksilver\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Varun\\Quicksilver\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Varun\\Quicksilver\\QuicksilverServiceProvider"
            ],
            "aliases": {
                "Quicksilver": "Varun\\Quicksilver\\Facades\\Quicksilver"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}